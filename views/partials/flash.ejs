<%  if (success.length > 0) { %>
    <header>
        <div class="alert alert-primary mb-0 text-center" role="alert">
            <%= success %>
        </div>
    </header>
    <% } %> 

    <%  if (failure.length > 0) { %>
    <header>
        <div class="alert alert-warning mb-0 text-center" role="alert">
            <%= failure %>
        </div>
    </header>
    <% } %> 